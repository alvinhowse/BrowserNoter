{
  "manifest_version": 3,
  "name": "BrowserNoter",
  "version": "1.0",
  "description": "Take notes in your browser",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "noter.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
